{
  "name": "runcheck",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "vite build --watch",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:codegen": "playwright codegen",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "tailwind:build": "NODE_ENV=production npx tailwindcss build ./src/tailwind.css -o ./dist/tailwind.css",
    "tailwind:watch": "npx tailwindcss build ./src/tailwind.css -o ./dist/tailwind.css --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.18",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/vue-query": "^4.29.14",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "youtube-player": "^5.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-docs": "^7.0.22",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.22",
    "@storybook/jest": "^0.1.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.0.22",
    "@storybook/vue3-vite": "^7.0.22",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@types/youtube-player": "^5.5.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.22",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
